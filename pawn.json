{
    "user": "KashCherry",
    "repo": "Timer-Fix-plugin",
    "resources": [
        {
            "name": "^timerfix-(.+)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": [""],
            "plugins": ["timerfix.dll"]
        },
        {
            "name": "^timerfix-(.+)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": [""],
            "plugins": ["timerfix.so"]
        }
    ],
    "runtime": {
        "plugins": ["KashCherry/Timer-Fix-plugin"]
    }
}
